### BEGIN /etc/grub.d/30_os-prober ###
menuentry 'Windows 10 (на /dev/sdb1)' --class windows --class os $menuentry_id_option 'osprober-chain-36FA1133FA10F13B' {
	insmod part_msdos
	insmod ntfs
	set root='hd1,msdos1'
	if [ x$feature_platform_search_hint = xy ]; then
	  search --no-floppy --fs-uuid --set=root --hint-bios=hd1,msdos1 --hint-efi=hd1,msdos1 --hint-baremetal=ahci1,msdos1  36FA1133FA10F13B
	else
	  search --no-floppy --fs-uuid --set=root 36FA1133FA10F13B
	fi
	parttool ${root} hidden-
	drivemap -s (hd0) ${root}
	chainloader +1
}
